{
  "name": "ts-sdk-client",
  "version": "1.0.0",
  "description": "TypeScript SDK client library with multiple HTTP implementations (Axios, Fetch, Gateway)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "example": "node examples/commonjs-usage.js",
    "clean": "rm -rf dist",
    "test": "npm run build && npm run test:all",
    "test:all": "npm run test:core && npm run test:axios && npm run test:fetch && npm run test:gateway && npm run test:environment",
    "test:core": "node tests/http-builder-core.test.js",
    "test:axios": "node tests/axios-http-builder.test.js",
    "test:fetch": "node tests/fetch-http-builder.test.js",
    "test:gateway": "node tests/gateway-http-builder.test.js",
    "test:environment": "node tests/environment-compatibility.test.js",
    "test:errors": "node tests/error-scenarios.test.js",
    "test:integration": "node tests/integration-real-services.test.js",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "typescript",
    "sdk",
    "client",
    "http",
    "axios",
    "fetch",
    "gateway-ts-sdk"
  ],
  "author": "Dexx Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "gateway-ts-sdk": "file:../gateway-ts-sdk"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
