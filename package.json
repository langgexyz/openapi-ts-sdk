{
  "name": "openapi-ts-sdk",
  "version": "2.0.0",
  "description": "OpenAPI TypeScript SDK core package - provides base HTTP builder abstractions and interfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "npm run build && npm run test:core",
    "test:core": "node tests/http-builder-core.test.js",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "openapi",
    "typescript",
    "sdk",
    "core",
    "http",
    "builder",
    "abstract"
  ],
  "author": "langgexyz",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
